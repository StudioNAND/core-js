    "type": "git",
    "url": "https://github.com/StudioNAND/core-js"
  },
  "devDependencies": {
    "@studionand/eslint-config": "0.0.3",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.2.2",
    "documentation": "^5.4.0",
    "lerna": "^3.19.0",
    "regenerator-runtime": "^0.11.1"
  }
}
